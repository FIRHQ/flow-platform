<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.flow.platform.domain">
    <class name="CmdResult" table="CmdResult">
        <id name="cmdId" type="string" column="CMD_ID">
            <generator class="assigned"/>
        </id>

        <property name="processId" not-null="false" column="PROCESS_ID"/>
        <property name="exitValue" not-null="false" column="EXIT_VALUE"/>
        <property name="duration" not-null="false" column="DURATION"/>
        <property name="totalDuration" not-null="false" column="TOTAL_DURATION"/>
        <property name="startTime" not-null="false" column="START_TIME"/>
        <property name="executedTime" not-null="false" column="EXECUTED_TIME"/>
        <property name="finishTime" not-null="false" column="FINISH_TIME"/>
    </class>
</hibernate-mapping>